{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
        "error",
        "unix"
    ],
    "semi": [
        "error",
        "never"
    ],
    "no-console": 0,
    "react/jsx-filename-extension": [
      1,
      { 
        "extensions": [
          ".js", ".jsx"
        ]
      }
    ]
  }
}